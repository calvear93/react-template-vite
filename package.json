		"start:release": "env -e release -m debug : vite",
		"build:dev": "wireit",
		"build:release": "wireit",
		"test:dev": "env -e dev -m test : vitest",
		"test:release": "env -e release -m test : vitest",
		"env:schema": "wireit",
		"lint": "wireit",
		"format": "wireit",
		"clean": "pnpx del-cli **/node_modules **/dist **/.reports **/*.tsbuildinfo **/.wireit",
		"preview": "wireit"
	},
