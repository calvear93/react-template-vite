		"eslint-plugin-regexp": "^1.15.0",
		"eslint-plugin-sonarjs": "^0.21.0",
		"eslint-plugin-testing-library": "^6.0.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"eslint-plugin-unicorn": "^48.0.1",
		"eslint-plugin-vitest": "^0.3.1",
		"jsdom": "^22.1.0",
		"postcss": "^8.4.29",
		"prettier": "^3.0.3",
