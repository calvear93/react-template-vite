		"@types/node": "^20.6.0",
		"@types/react": "^18.2.21",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.6.0",
		"@typescript-eslint/parser": "^6.6.0",
		"@unocss/eslint-config": "^0.55.7",
		"@vitejs/plugin-react-swc": "^3.3.2",
		"@vitest/coverage-v8": "^0.34.4",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
