		"vite-plugin-checker": "^0.6.2",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-pwa": "^0.16.5",
		"vite-plugin-svgr": "^3.2.0",
		"vite-plugin-webfont-dl": "^3.7.6",
		"vite-plugin-windicss": "^1.9.1",
		"vite-tsconfig-paths": "^4.2.0",
		"vitest": "^0.34.4",
		"vitest-mock-extended": "^1.2.1",
		"wireit": "^0.13.0"
	},
	"wireit": {
		"build:dev": {
			"command": "env -e dev -m build --log debug : vite build",
			"clean": "if-file-deleted",
			"output": [
				"dist/**"
			],
			"files": [
				"src/**",
				"public/**",
				"index.html",
				"tsconfig.json",
				"vite.config.ts",
				"env/appsettings.json",
				"env/dev.env.json",
				"!**/*.{spec,test}.*",
				"!**/__{tests,mocks,fixtures}__/*"
			]
		},
		"build:release": {
			"command": "env -e dev -m build --log debug : vite build",
			"clean": "if-file-deleted",
			"output": [
				"dist/**"
			],
			"files": [
				"src/**",
				"public/**",
				"index.html",
				"tsconfig.json",
				"vite.config.ts",
				"env/appsettings.json",
				"env/release.env.json",
				"!**/*.{spec,test}.*",
				"!**/__{tests,mocks,fixtures}__/*"
			]
		},
		"preview": {
			"command": "vite preview",
			"dependencies": [
				"build:dev"
			],
			"env": {
				"BASE_URL": "/"
			}
		},
		"format": {
			"command": "prettier --cache --write **/*.{ts,cts,mts,tsx,css,scss,sass,json,html,md,yml,yaml,webmanifest}",
			"output": [
				"./node_modules/.cache/prettier/.prettier-cache"
			],
			"files": [
				".prettierrc",
				".prettierignore",
				"**/*.{ts,cts,mts,tsx,css,scss,sass,json,html,md,yml,yaml,webmanifest}"
			]
		},
		"lint": {
			"command": "eslint --cache src/**/*.{ts,cts,mts,tsx}",
			"output": [
				".eslintcache"
			],
			"files": [
				".eslintrc",
				".eslintignore",
				"src/**/*.{ts,cts,mts,tsx}"
			],
			"dependencies": [
				"stylelint"
			]
		},
		"stylelint": {
			"command": "stylelint --cache src/**/*.{css,scss,sass}",
			"output": [
				".stylelintcache"
			],
			"files": [
				".stylelintrc",
				".stylelintignore",
				"src/**/*.{css,scss,sass}"
			]
		},
		"env:schema": {
			"command": "env schema -e dev -m build",
			"output": [
				"env/settings/schema.json"
			],
			"files": [
				"env/appsettings.json",
				"env/*.env.json"
			]
		}
	},
	"engines": {
		"node": ">=18",
		"npm": ">=8",
		"pnpm": ">=7"
	},
	"browserslist": {
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		],
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all",
			"not ie <= 10"
		]
	}
}
