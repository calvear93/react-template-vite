{
	"version": "0.0.1",
	"project": "(((project-name)))",
	"name": "(((app-name)))",
	"title": "(((app-title)))",
	"description": "(((project-description)))",
	"scripts": {
		"start:dev": "env -e dev -m debug --log debug : vite",
		"start:release": "env -e release -m debug : vite",
		"build:dev": "wireit",
		"build:release": "wireit",
		"test:dev": "env -e dev -m test : vitest",
		"test:release": "env -e release -m test : vitest",
		"test:mutation": "env -e dev -m test : stryker run",
		"env:schema": "wireit",
		"abc": "stylelint --cache src/**/*.{css,scss,sass}",
		"lint": "wireit",
		"format": "wireit",
		"clean": "pnpx del-cli **/node_modules **/dist **/.reports **/*.tsbuildinfo **/.wireit",
		"preview": "wireit"
	},
	"license": "MIT",
	"private": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/calvear93/react-template-vite"
