{
	"version": "0.0.1",
	"project": "(((project-name)))",
	"name": "(((app-name)))",
	"title": "(((app-title)))",
	"description": "(((project-description)))",
	"scripts": {
		"start:dev": "env -e dev -m debug --log debug : vite",
		"start:release": "env -e release -m debug : vite",
		"build:dev": "wireit",
		"build:release": "wireit",
		"test:dev": "env -e dev -m test : vitest",
		"test:release": "env -e release -m test : vitest",
		"test:mutation": "env -e dev -m test : stryker run",
		"env:schema": "wireit",
		"abc": "stylelint --cache src/**/*.{css,scss,sass}",
		"lint": "wireit",
		"format": "wireit",
		"clean": "pnpx del-cli dist reports **/*.tsbuildinfo .wireit .swc .node_modules",
		"preview": "wireit"
	},
	"license": "MIT",
	"repository": "https://github.com/calvear93/react-template-vite",
	"keywords": [
		"react",
		"pnpm",
		"vite",
		"vitest",
		"typescript",
		"spa",
		"pwa",
		"unocss",
		"tailwind"
	],
	"type": "module",
	"dependencies": {
		"jotai": "2.6.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router-dom": "6.20.1"
	},
	"devDependencies": {
		"@calvear/env": "2.9.1",
		"@stryker-mutator/core": "8.0.0",
		"@stryker-mutator/typescript-checker": "8.0.0",
		"@stryker-mutator/vitest-runner": "8.0.0",
		"@testing-library/dom": "9.3.3",
		"@testing-library/react": "14.1.2",
		"@testing-library/user-event": "14.5.1",
		"@types/node": "20.10.4",
		"@types/react": "18.2.43",
		"@types/react-dom": "18.2.17",
		"@typescript-eslint/eslint-plugin": "6.14.0",
		"@typescript-eslint/parser": "6.14.0",
		"@unocss/eslint-config": "0.58.0",
		"@vitejs/plugin-react-swc": "3.5.0",
		"@vitest/coverage-v8": "1.0.4",
		"eslint": "8.55.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-import": "2.29.0",
		"eslint-plugin-jsx-a11y": "6.8.0",
		"eslint-plugin-perfectionist": "2.5.0",
		"eslint-plugin-prettier": "5.0.1",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-react": "7.33.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-redos": "4.4.5",
		"eslint-plugin-regexp": "2.1.2",
		"eslint-plugin-sonarjs": "0.23.0",
		"eslint-plugin-testing-library": "6.2.0",
		"eslint-plugin-tsdoc": "0.2.17",
		"eslint-plugin-unicorn": "49.0.0",
		"eslint-plugin-vitest": "0.3.16",
		"jsdom": "23.0.1",
		"postcss": "8.4.32",
		"prettier": "3.1.1",
		"sass": "1.69.5",
		"stylelint": "16.0.2",
		"stylelint-config-rational-order": "0.1.2",
		"stylelint-config-recommended": "14.0.0",
		"stylelint-config-standard": "35.0.0",
		"stylelint-config-standard-scss": "12.0.0",
		"stylelint-order": "6.0.4",
		"stylelint-prettier": "5.0.0",
		"stylelint-scss": "6.0.0",
		"tslib": "2.6.2",
		"typescript": "5.3.3",
		"unocss": "0.58.0",
		"vite": "5.0.7",
		"vite-plugin-checker": "0.6.2",
		"vite-plugin-pwa": "0.17.4",
		"vite-plugin-webfont-dl": "3.9.1",
		"vite-tsconfig-paths": "4.2.2",
		"vitest": "1.0.4",
		"vitest-mock-extended": "1.3.1",
		"wireit": "0.14.1"
	},
	"wireit": {
		"build:dev": {
			"command": "env -e dev -m build --log debug : vite build",
			"clean": "if-file-deleted",
			"output": [
				"dist/**"
			],
			"files": [
				"src/**",
				"public/**",
				"index.html",
				"tsconfig.json",
				"vite.config.ts",
				"env/appsettings.json",
				"env/dev.env.json",
				"!**/*.{spec,test}.*",
				"!**/__{tests,mocks,fixtures}__/*"
			]
		},
		"build:release": {
			"command": "env -e dev -m build --log debug : vite build",
			"clean": "if-file-deleted",
			"output": [
				"dist/**"
			],
			"files": [
				"src/**",
				"public/**",
				"index.html",
				"tsconfig.json",
				"vite.config.ts",
				"env/appsettings.json",
				"env/release.env.json",
				"!**/*.{spec,test}.*",
				"!**/__{tests,mocks,fixtures}__/*"
			]
		},
		"preview": {
			"command": "vite preview",
			"dependencies": [
				"build:dev"
			],
			"env": {
				"BASE_URL": "/"
			}
		},
		"format": {
			"command": "prettier --cache --write **/*.{ts,cts,mts,tsx,css,scss,sass,json,html,md,yml,yaml,webmanifest}",
			"output": [
				"./node_modules/.cache/prettier/.prettier-cache"
			],
			"files": [
				".prettierrc",
				".prettierignore",
				"**/*.{ts,cts,mts,tsx,css,scss,sass,json,html,md,yml,yaml,webmanifest}"
			]
		},
		"lint": {
			"command": "eslint --cache src/**/*.{ts,cts,mts,tsx}",
			"output": [
				".eslintcache"
			],
			"files": [
				".eslintrc",
				".eslintignore",
				"src/**/*.{ts,cts,mts,tsx}"
			],
			"dependencies": [
				"stylelint"
			]
		},
		"stylelint": {
			"command": "stylelint --cache src/**/*.{css,scss,sass}",
			"output": [
				".stylelintcache"
			],
			"files": [
				".stylelintrc",
				".stylelintignore",
				"src/**/*.{css,scss,sass}"
			]
		},
		"env:schema": {
			"command": "env schema -e dev -m build",
			"output": [
				"env/settings/schema.json"
			],
			"files": [
				"env/appsettings.json",
				"env/*.env.json"
			]
		}
	},
	"engines": {
		"node": ">=20",
		"npm": ">=9",
		"pnpm": ">=8"
	},
	"browserslist": {
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		],
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all",
			"not ie <= 10"
		]
	}
}
