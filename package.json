{
	"$schema": "https://json.schemastore.org/package.json",
	"version": "0.0.1",
	"project": "(((project-name)))",
	"name": "(((app-name)))",
	"title": "(((app-title)))",
	"description": "(((app-description)))",
	"scripts": {
		"start:dev": "env -e dev -m debug --log debug : vite",
		"start:release": "env -e release -m debug : vite",
		"build:dev": "env -e dev -m build --log debug : vite build",
		"build:release": "env -e dev -m build --log debug : vite build",
		"test:dev": "env -e dev -m test : vitest",
		"test:release": "env -e release -m test : vitest",
		"test:mutation": "env -e dev -m test : stryker run",
		"env:schema": "env schema -e dev -m build",
		"lint": "eslint --cache --cache-location node_modules/.cache/eslint/",
		"stylelint": "stylelint --cache src/**/*.css --cache-location node_modules/.cache/stylelint/",
		"format": "prettier --cache --write **/*.{ts,cts,mts,tsx,css,json,html,md,yml,yaml,webmanifest}",
		"clean": "pnpx del-cli dist reports **/*.tsbuildinfo .swc .node_modules",
		"preview": "pnpm build:dev & env -e dev -m build : vite preview"
	},
	"imports": {
		"#libs/ioc": "./src/libs/ioc/ioc.builder.tsx",
		"#libs/router": "./src/libs/router/index.ts",
		"#libs/feature": "./src/libs/feature/index.ts"
	},
	"dependencies": {
		"jotai": "2.13.1",
		"react": "19.1.1",
		"react-dom": "19.1.1",
		"react-router": "7.8.1",
		"zod": "4.0.17"
	},
	"devDependencies": {
		"@calvear/env": "2.10.3",
		"@iconify-json/mdi": "1.2.3",
		"@stryker-mutator/core": "9.0.1",
		"@stryker-mutator/typescript-checker": "9.0.1",
		"@stryker-mutator/vitest-runner": "9.0.1",
		"@testing-library/dom": "10.4.1",
		"@testing-library/jest-dom": "6.8.0",
		"@testing-library/react": "16.3.0",
		"@testing-library/user-event": "14.6.1",
		"@types/eslint": "9.6.1",
		"@types/node": "24.3.0",
		"@types/react": "19.1.10",
		"@types/react-dom": "19.1.7",
		"@unocss/eslint-config": "66.4.2",
		"@vitejs/plugin-react-swc": "4.0.1",
		"@vitest/coverage-v8": "3.2.4",
		"eslint": "9.33.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-jest-dom": "5.5.0",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-perfectionist": "4.15.0",
		"eslint-plugin-prettier": "5.5.4",
		"eslint-plugin-promise": "7.2.1",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-regexp": "2.10.0",
		"eslint-plugin-sonarjs": "3.0.4",
		"eslint-plugin-testing-library": "7.6.6",
		"eslint-plugin-unicorn": "60.0.0",
		"eslint-plugin-vitest": "0.5.4",
		"globals": "16.3.0",
		"happy-dom": "18.0.1",
		"postcss": "8.5.6",
		"prettier": "3.6.2",
		"stylelint": "16.23.1",
		"stylelint-config-rational-order": "0.1.2",
		"stylelint-config-recommended": "17.0.0",
		"stylelint-config-standard": "39.0.0",
		"stylelint-order": "7.0.0",
		"stylelint-prettier": "5.0.3",
		"stylelint-use-nesting": "6.0.0",
		"tslib": "2.8.1",
		"typescript": "5.9.2",
		"typescript-eslint": "8.40.0",
		"unocss": "66.4.2",
		"vite": "7.1.3",
		"vite-plugin-checker": "0.10.2",
		"vite-plugin-pwa": "1.0.3",
		"vite-plugin-webfont-dl": "3.11.1",
		"vitest": "3.2.4",
		"vitest-mock-extended": "3.1.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@parcel/watcher",
			"@swc/core",
			"esbuild"
		]
	},
	"type": "module",
	"engines": {
		"node": ">=22",
		"npm": ">=10",
		"pnpm": ">=10"
	},
	"license": "MIT",
	"repository": "https://github.com/calvear93/react-template-vite",
	"keywords": [
		"react",
		"pnpm",
		"vite",
		"vitest",
		"typescript",
		"spa",
		"pwa",
		"unocss"
	]
}
