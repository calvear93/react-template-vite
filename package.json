		"wireit": "^0.9.5"
	},
	"wireit": {
		"build:dev": {
			"command": "env -e dev -m build --log debug : vite build",
			"clean": "if-file-deleted",
			"output": [
				"dist/**"
			],
			"files": [
				"src/**",
				"public/**",
				"index.html",
				"tsconfig.json",
				"vite.config.ts",
				"env/appsettings.json",
				"env/dev.env.json",
				"!**/*.{spec,test}.*",
				"!**/__{tests,mocks,fixtures}__/*"
			]
		},
		"build:release": {
			"command": "env -e dev -m build --log debug : vite build",
			"clean": "if-file-deleted",
			"output": [
				"dist/**"
			],
			"files": [
				"src/**",
				"public/**",
				"index.html",
				"tsconfig.json",
				"vite.config.ts",
				"env/appsettings.json",
				"env/release.env.json",
				"!**/*.{spec,test}.*",
				"!**/__{tests,mocks,fixtures}__/*"
			]
		},
		"preview": {
			"command": "vite preview",
			"dependencies": [
				"build:dev"
			],
			"env": {
				"BASE_URL": "/"
			}
		},
		"format": {
			"command": "prettier --cache --write **/*.{ts,cts,mts,tsx,css,scss,sass,json,html,md,yml,yaml,webmanifest}",
			"output": [
				"./node_modules/.cache/prettier/.prettier-cache"
			],
			"files": [
				".prettierrc",
				".prettierignore",
				"**/*.{ts,cts,mts,tsx,css,scss,sass,json,html,md,yml,yaml,webmanifest}"
			]
		},
		"lint": {
			"command": "eslint --cache src/**/*.{ts,cts,mts,tsx}",
			"output": [
				".eslintcache"
			],
			"files": [
				".eslintrc",
				".eslintignore",
				"src/**/*.{ts,cts,mts,tsx}"
			],
			"dependencies": [
				"stylelint"
			]
		},
		"stylelint": {
			"command": "stylelint --cache src/**/*.{css,scss,sass}",
			"output": [
				".stylelintcache"
			],
			"files": [
				".stylelintrc",
				".stylelintignore",
				"src/**/*.{css,scss,sass}"
			]
		},
		"env:schema": {
			"command": "env schema -e dev -m build",
			"output": [
				"env/settings/schema.json"
			],
			"files": [
				"env/appsettings.json",
				"env/*.env.json"
			]
		}
	},
	"engines": {
		"node": ">=18",
		"npm": ">=8",
		"pnpm": ">=7"
	},
	"browserslist": {
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		],
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all",
			"not ie <= 10"
		]
	}
}
